<div id="inmueble-data" class="ubicacion" data-lat="{{latitud}}" data-lng="{{longitud}}" data-title1="{{titulo1}}"
  data-title2="{{titulo2}}" data-img="{{imagen_chica}}" data-comentario="{{comentario1}}"></div>
<div class="ubicacion" id="inmueble-descripcion">
  <div class="container">
    <div class="row no-gutters">
      <div class="col-xs-12 col-md-5">
        <div class="inmueble-izquierdo">
          <div class="row">
            <div class="col-12">
              <p class="titulo">{{titulo1}} {{titulo2}}</p>
              <p class="subtitulo-1">{{subtitulo}}</p>
              <p class="comentario-1">{{comentario-1}}{{comentario-2}}</p>
              {{#each detalles}}
              <div class="row">
                {{!-- <div class="col-4">
                  <p class="inmueble-1">{{nombre}}</p>
                </div> --}}
                <div class="col-12">
                  <p class="inmueble-2"><span><img src="{{icon}}" alt="" class="descripcion-icon"></span>{{texto}}</p>
                </div>
              </div>
              {{/each}}
              {{#if mostrar_caracteristicas_inmueble}}
              <div class="row">
                <div class="col-12">
                  <p class="subtitulo-2">CARACTER√çSTICAS GENERALES</p>
                  {{#each caracteristicas_casa}}
                  <li>
                    <p class="comentario-2">{{caracteristica}}</p>
                  </li>
                  {{/each}}
                </div>
              </div>
              {{/if}}

              {{#if mostrar_caracteristicas_fraccionamiento}}

              <div class="row">
                <div class="col-12">
                  <p class="subtitulo-3">SOBRE EL FRACCIONAMIENTO</p>
                  {{#each caracteristicas_fracc}}
                  <li>
                    <p class="comentario-2">{{caracteristica}}</p>
                  </li>
                  {{/each}}
                </div>
              </div>
              {{/if}}
            </div>
          </div>
        </div>
      </div>
      <div class="col-xs-12 col-md-7">
        <div class="inmueble-derecho">
          <div class="row">
            <div class="col-12">
              <div id="galeria-detalle" class="carousel slide" data-ride="carousel">
                <ol class="carousel-indicators">
                  {{#each imagenes}}
                  <li data-target="#galeria-detalle" data-slide-to="{{@index}}" class="{{#if @first}}active{{/if}}"></li>
                  {{/each}}

                </ol>
                <div class="carousel-inner">
                  {{#each imagenes}}
                  <div class="carousel-item {{#if @first}}active{{/if}}">
                    <a href="{{imagen}}" data-fancybox="galeria-detalle-inmueble">
                      <img class="d-block inmueble-imagen" src="{{imagen}}" alt="">
                    </a>
                  </div>
                  {{/each}}

                </div>
                <a class="carousel-control-prev" href="#galeria-detalle" role="button" data-slide="prev">
                  <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                  <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#galeria-detalle" role="button" data-slide="next">
                  <span class="carousel-control-next-icon" aria-hidden="true"></span>
                  <span class="sr-only">Next</span>
                </a>
              </div>

            </div>
          </div>
          <div class="row">
            <div class="col-xs-12 col-md-6">
              <div class="row">

                <a href="https://www.facebook.com/dialog/feed?app_id=184683071273&link=" target="_blank" class="inmueble-share inmueble-social facebook">
                  <img src="/assets/img/social-f.svg" alt="">
                </a>
                <a href="https://twitter.com/home?status=" target="_blank" class="inmueble-share inmueble-social twitter">
                  <img src="/assets/img/social-t.svg" alt="">
                </a>
                <a href="whatsapp://send" data-action="share/whatsapp/share" class=" inmueble-social whatsapp">
                  <img src="/assets/img/social-w.svg" alt="">
                </a>
                <a class="inmueble-social share">
                  <img src="/assets/img/social-s.svg" alt="">
                </a>
                <a href="mailto:?&body=" target="_blank" class="inmueble-share inmueble-social mail">
                  <img src="/assets/img/social-m.svg" alt="">
                </a>

              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  {{#if mostrar_planos}}
  <div class="inmueble-galeria">
    <div class="container planos">
      <div class="row">
        <div class="col-12">
          <p class="titulo">PLANOS DE LA CASA</p>
          <div class="galeria">
            <div id="galeria-planos" class="carousel slide" data-ride="carousel">
              <ol class="carousel-indicators">
                {{#each planos}}
                <li data-target="#galeria-planos" data-slide-to="{{@index}}" class="{{#if @first}}active{{/if}}"></li>
                {{/each}}
              </ol>
              <div class="carousel-inner">
                {{#each planos}}
                <div class="carousel-item {{#if @first}}active{{/if}}">
                  <a href="{{plano}}" data-fancybox="galeria-planos-inmueble">
                    <img class="d-block w-100 plano" src="{{plano}}" alt="">
                  </a>
                </div>
                {{/each}}

              </div>
              <a class="carousel-control-prev" href="#galeria-planos" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
              </a>
              <a class="carousel-control-next" href="#galeria-planos" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
              </a>
            </div>

          </div>
        </div>
      </div>
    </div>
  </div>
  {{/if}}
</div>